var primeiranota = Number (prompt ("Digite nota do primeiro trismestre") );
console.log(primeiranota)


var segundanota = Number (prompt("Digite nota do segundo trimestre") );
console.log(segundanota)

var terceiranota = Number (prompt("Digite nota do terceira trimestre") );
console.log(terceiranota)

console.log(   (  primeiranota + segundanota + terceiranota ) /3 );

alert("sua media e" + (primeiranota + segundanota + terceiranota) /3 ); 
